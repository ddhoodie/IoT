      "title": "DHT Sensor (Temperature & Humidity)",
      "type": "timeseries",
      "targets": [
        {
          "datasource": {
            "type": "influxdb",
            "uid": "InfluxDB_v2"
          },
          "query": "from(bucket: \"dht\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"DHT\")\n  |> filter(fn: (r) => r[\"_field\"] == \"temperature\" or r[\"_field\"] == \"humidity\")\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)\n  |> yield(name: \"mean\")",
          "refId": "A"
        }
      ]
